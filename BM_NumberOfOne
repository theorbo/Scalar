/* Write a function that takes an integer and returns the number of 1 bits it has. */

public class Solution {
    public int numSetBits(int A) {

        int count = 0;
        for(int i = 0; i < 32; i++){
           if((1&(A >> i)) == 1)
            count++; 
        }
        return count;
    }
}
